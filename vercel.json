{
    "version": 2,
    "env": {
      "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }  